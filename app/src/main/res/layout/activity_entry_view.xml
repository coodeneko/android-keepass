<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.joelkreutzwieser.apps.keepass.EntryViewActivity">

    <include
        android:id="@+id/app_bar"
        layout="@layout/app_bar" />

    <fragment
        android:id="@+id/propertyList"
        android:name="com.joelkreutzwieser.apps.keepass.EntryViewFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/app_bar"
        android:layout="@layout/fragment_entry_view"
        tools:layout="@layout/fragment_entry_view" />

</RelativeLayout>
